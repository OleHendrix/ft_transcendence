generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model Account {
  id       Int    @id @default(autoincrement())
  email    String @unique
  username String @unique
  password String
  wins     Int    @default(0)
  draws    Int    @default(0)
  loses    Int    @default(0)
}
